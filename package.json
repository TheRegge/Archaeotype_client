{
	"name": "archaeotype-client",
	"version": "2.1.2",
	"description": "A virtual archaeological dig. Created with phaser3-parcel-template, a typescript template project for Phaser 3 using Parceljs",
	"scripts": {
		"start": "NODE_ENV=development parcel src/index.html -p 3001",
		"build": "NODE_ENV=production parcel build src/index.html --public-url ./ --no-source-maps",
		"test": "echo \"Error: no test specified\" && exit 1",
		"lint": "eslint ./src --ext .js,.jsx,.ts,.tsx"
	},
	"author": "supertommy",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/ourcade/phaser3-parcel-template.git"
	},
	"homepage": "https://github.com/ourcade/phaser3-parcel-template",
	"devDependencies": {
		"@parcel/packager-raw-url": "^2.7.0",
		"@parcel/transformer-webmanifest": "^2.7.0",
		"@types/node": "^17.0.33",
		"@typescript-eslint/eslint-plugin": "^2.29.0",
		"@typescript-eslint/parser": "^2.29.0",
		"cssnano": "^5.0.12",
		"eslint": "^6.8.0",
		"minimist": "^1.2.5",
		"parcel": "^2.0.0-rc.0",
		"parcel-reporter-static-files-copy": "^1.3.4",
		"postcss": "^8.4.4",
		"prettier": "^2.5.1",
		"typedoc": "^0.22.10",
		"typescript": "^4.5.4"
	},
	"dependencies": {
		"@parcel/transformer-sass": "^2.7.0",
		"awesome-notifications": "^3.1.3",
		"axios": "^0.26.1",
		"dotenv": "^16.0.1",
		"jose": "^4.6.1",
		"phaser": "^3.55.2"
	},
	"parcelCleanPaths": [
		"dist"
	],
	"staticFiles": {
		"staticPath": "public",
		"watcherGlob": "**"
	}
}